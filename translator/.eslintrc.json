{
	"parserOptions": {
		"ecmaVersion": 6,
		"sourceType": "module",
		"ecmaFeatures": {
			"jsx": true
		}
	},
	"rules": {
		"semi": 2,
		"eqeqeq": 2,
		"no-dupe-args":2,
		"no-dupe-keys":2,
		"no-duplicate-case":2,
		"no-empty":2,
		"no-extra-parens":2,
		"no-extra-semi":2,
		"no-invalid-regexp":2,
		"no-unreachable":2,
		"curly":2,
		"no-caller":2,
		"no-empty-function":2,
		"no-eval":2,
		"no-new-wrappers":2,
		"no-self-assign":2,
		"strict":2,
		"init-declarations":2,
		"no-unused-vars":2,
		"global-require":2,
		"no-mixed-spaces-and-tabs":2,
		"no-var":2,
		"no-const-assign":2,
		"no-dupe-class-members":2,
		"prefer-const":2,
		"no-undef":2,
		"no-undefined":2,
		"no-undef-init":2,
		"no-delete-var":2,
		"no-unused-vars":2
	}
}